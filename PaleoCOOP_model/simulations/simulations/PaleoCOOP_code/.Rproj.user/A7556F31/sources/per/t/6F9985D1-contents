                                TienShan_Scenario_3_run22_results_plot,
                                TienShan_Scenario_3_run25_results_plot,
                                top = textGrob("Scenario 3 Tian Shan, prob.coop: 0.20", gp = gpar(fontsize = unit(20, "lines"))), nrow = 3)


scenario3_prob20


